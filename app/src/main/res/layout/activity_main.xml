<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg2"

        tools:context=".MainActivity"
        >



    <TextView
            android:textSize="32sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:textColor="#ECE7E7"
            app:layout_constraintTop_toBottomOf="@+id/textView1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

    <TextView
            android:text="@string/total_case"
            android:layout_width="wrap_content"
            android:id="@+id/textView1"
            android:fontFamily="@font/aquire"
            android:layout_height="wrap_content"
            android:textColor="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintTop_toTopOf="@+id/guideline"
            android:layout_marginTop="5dp"
            />

    <TextView
            android:text="@string/Heading"
            android:textSize="25sp"
            android:textColor="#BD4848"
            android:fontFamily="@font/aquire_b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            />

    <TextView
            android:text="@string/recovered"
            android:textSize="10sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/aquire"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView5"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toTopOf="@+id/guideline2"
            />

    <TextView
            android:text="@string/death"
            android:textSize="10sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/aquire"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toTopOf="@+id/guideline2"
            />

    <TextView
            android:text="TextView"
            android:textSize="25sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/cambo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView7"
            />

    <TextView

            android:text="TextView"
            android:textSize="25sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/cambo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent"
            />

    <TextView
            android:text="TextView"
            android:textSize="11sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/cambo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView9"
            />

    <TextView
            android:text="TextView"
            android:textSize="11sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/cambo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="parent"
            />

    <TextView
            android:text="TextView"
            android:textSize="11sp"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/cambo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="3dp"
            />

    <TextView
            android:text="@string/country"
            android:textSize="14sp"
            android:fontFamily="@font/aquire"
            android:textColor="#1B1FD5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toTopOf="@+id/guideline3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="55dp"
            />

    <TextView
            android:textSize="14sp"
            android:textColor="#1B1FD5"
            android:fontFamily="@font/hashedbrowns"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            app:layout_constraintTop_toTopOf="@+id/guideline3"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="30dp"
            />

    <Button
            android:text="Button"
            android:background="@drawable/button"
            android:textColor="#ffff"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toTopOf="@+id/guideline4"
            />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline"

            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.146"
            />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline2"

            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.31"
            />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline3"

            android:orientation="horizontal"

            app:layout_constraintGuide_percent="0.44458598"
            />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline4"

            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5"
            />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline5"

            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6"
            />

    <com.github.mikephil.charting.charts.LineChart
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/linechart1"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="UnknownId"
            app:layout_constraintHorizontal_bias="0.512"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toTopOf="@+id/guideline5"
            />


</androidx.constraintlayout.widget.ConstraintLayout>
